<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %m%n %ex</pattern>
        </encoder>
    </appender>

    <logger name="skunk" level="TRACE"/>
    <logger name="http4s"/>

    <root level="WARNING">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>
